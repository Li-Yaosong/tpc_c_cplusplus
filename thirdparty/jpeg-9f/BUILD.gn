import("//build/ohos.gni")  
group("jpeg-9f"){
    deps = [
        "//third_party/jpeg-9f:cjpeg",
        "//third_party/jpeg-9f:djpeg",
        "//third_party/jpeg-9f:jpegtran",
        "//third_party/jpeg-9f:rdjpgcom",
        "//third_party/jpeg-9f:wrjpgcom",
        "//third_party/jpeg-9f:libjpeg_static",
    ]
}




config("libiconv_nowarn_config"){
    cflags = [
       
    ]
}


config("libiconv_cflag_config"){
    cflags = [
      "-g",
      "-O2"
    ]
    ldflags = [
         
    ]
}

ohos_executable("wrjpgcom") {
  sources = [
    "wrjpgcom.c",
  ]

  
