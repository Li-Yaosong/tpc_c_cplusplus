source HPKBUILD > /dev/null 2>&1
logfile=${LYCIUM_THIRDPARTY_ROOT}/${pkgname}/${pkgname}_${ARCH}_${OHOS_SDK_VER}_test.log

openharmonycheck() {
    res=0
    cd $builddir/bin
    #执行单例，或者执行整个测试脚本rapidjson_test.sh测试整个测试文件，
    #这里以单例为例，本地测试会打印二进制文件不兼容，需要在对应鸿蒙设备进行测试。
    ./archivertest > ${logfile} 2>&1
    res=$?
    cd $OLDPWD
    return $resll
}
